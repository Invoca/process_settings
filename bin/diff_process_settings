#!/bin/sh

set -e

if [ "$1" == "" ]; then
  echo "usage: $0 <path-to-combined_process_settings.yml>"
  exit 1
fi

if [ ! -f "$1" ]; then
  echo "$1 not found--must be a path to combined_process_settings.yml"
  exit 1
fi

rm -f                  tmp/previous-combined_process_settings.yml
git show deployed:$1 > tmp/previous-combined_process_settings.yml

diff tmp/previous-combined_process_settings.yml $1
